# FieldForge Environment Variables
# Copy this file to .env and fill in your values

# Supabase Configuration
VITE_SUPABASE_URL=your-supabase-project-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Backend API
VITE_API_BASE_URL=https://your-backend-url.com

# Google Places API for Location Services
VITE_GOOGLE_PLACES_API_KEY=your-google-places-api-key
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Weather Service API (DTN or similar)
VITE_WEATHER_API_KEY=your-weather-api-key
VITE_WEATHER_API_URL=https://api.weather.com

# Mapbox for GIS Visualization (T-Line routing)
VITE_MAPBOX_TOKEN=your-mapbox-token

# File Storage (S3 or compatible)
VITE_STORAGE_URL=https://your-storage-url.com

# WebSocket for Real-time Updates
VITE_WEBSOCKET_URL=wss://your-websocket-url.com

# Environment Configuration
VITE_ENVIRONMENT=production
VITE_APP_NAME=FieldForge
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_VIDEO_CALLS=true
VITE_ENABLE_WEATHER_ALERTS=true

# Sentry Error Tracking (optional)
VITE_SENTRY_DSN=your-sentry-dsn

# Analytics (optional)
VITE_GA_TRACKING_ID=your-google-analytics-id
