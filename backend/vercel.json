{
  "framework": null,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "functions": {
    "dist/server.js": {
      "runtime": "@vercel/node@3.0.0",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/dist/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
